{
  "name": "led-panel-manager",
  "version": "0.1.0",
  "description": "Sistema completo de gerenciamento de pain√©is LED",
  "main": "src/main.tsx",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release:patch": "npm version patch && npm run build && git push --tags",
    "release:minor": "npm version minor && npm run build && git push --tags", 
    "release:major": "npm version major && npm run build && git push --tags",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare-release": "npm run test && npm run build && npm run changelog"
  },
  "keywords": [
    "led",
    "panel",
    "management",
    "react",
    "typescript",
    "vite",
    "tailwind"
  ],
  "author": "LED Panel Manager Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/[username]/led-panel-manager.git"
  },
  "bugs": {
    "url": "https://github.com/[username]/led-panel-manager/issues"
  },
  "homepage": "https://github.com/[username]/led-panel-manager#readme",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
